class Solution:
    def maxOperations(self, nums: List[int], k: int) -> int:
        m_op = 0
        nums.sort()
        l = 0
        r = len(nums)-1
        #print(nums)
        while l < r:
            if(nums[l] + nums[r] == k):
                l+=1
                r-=1
                m_op+=1
            elif(nums[l] + nums[r] > k):
                r-=1
            elif(nums[l] + nums[r] < k):
                l+=1
        return m_op
